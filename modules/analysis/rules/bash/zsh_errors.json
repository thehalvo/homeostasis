    {
      "id": "zsh_not_a_function",
      "pattern": "zsh: not a function",
      "type": "FunctionError",
      "description": "Attempted to call a name that is not a function",
      "root_cause": "bash_function_error",
      "suggestion": "Ensure the function is defined before calling it",
      "category": "bash",
      "severity": "medium",
      "confidence": "high",
