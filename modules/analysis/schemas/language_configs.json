    ],
    "severity_mappings": {
      "debug": "debug",
      "info": "info",
      "warning": "warn",
      "error": "error",
      "critical": "panic",
      "fatal": "fatal"
    }
  },
  "csharp": {
    "language": "csharp",
    "field_mappings": {
